<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.1.xsd">

  <suppress>
    <notes><![CDATA[Shadowed dependency from AppInsights]]></notes>
    <gav regex="true">^io\.netty:netty-tcnative-boringssl-static:2\.0\.17\.Final$</gav>
    <cve>CVE-2014-3488</cve>
    <cve>CVE-2015-2156</cve>
  </suppress>

  <suppress>
    <notes><![CDATA[No fix is available]]></notes>
    <gav regex="true">^org\.springframework\.security:spring-security-crypto:5\.1\.5\.RELEASE$</gav>
    <cve>CVE-2018-1258</cve>
  </suppress>

  <!-- not used by server -->

  <suppress>
    <notes><![CDATA[Pulled in by azure-servicebus. No newer version available]]></notes>
    <gav regex="true">^com\.microsoft\.azure:adal4j:1\.3\.0$</gav>
    <cve>CVE-2011-1068</cve>
  </suppress>

  <suppress>
    <notes><![CDATA[Used only for testing not used in prod]]></notes>
    <gav regex="true">^org\.testcontainers:postgresql:.*$</gav>
    <cpe>cpe:/a:postgresql:postgresql</cpe>
  </suppress>

  <suppress>
    <notes><![CDATA[ no fix available ]]></notes>
    <gav regex="true">^org.codehaus.groovy:groovy-(json|xml):2\.5\.7$</gav>
    <cve>CVE-2016-6497</cve>
  </suppress>

  <suppress>
    <notes><![CDATA[
       file name: wiremock-standalone-2..0.jar.
       This is only used for TESTING purposes.
   ]]></notes>
    <filePath regex="true">.*wiremock-standalone-.*\.jar.*</filePath>
    <cpe>cpe:/a:eclipse:jetty</cpe>
    <cpe>cpe:/a:fasterxml:jackson</cpe>
    <cpe>cpe:/a:fasterxml:jackson-databind</cpe>
    <cpe>cpe:/a:google:guava</cpe>
  </suppress>

  <suppress>
    <notes><![CDATA[ no fix available ]]></notes>
    <gav regex="true">^org\.springframework\.security:spring-security-rsa:1\.0\.7\.RELEASE$</gav>
    <cpe>cpe:/a:pivotal_software:spring_security</cpe>
    <cpe>cpe:/a:pivotal_software:spring_security_oauth</cpe>
    <cpe>cpe:/a:pivotal:spring_security_oauth</cpe>
    <cpe>cpe:/a:vmware:springsource_spring_security</cpe>
  </suppress>

</suppressions>
