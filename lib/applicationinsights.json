{
  "connectionString": "${file:/mnt/secrets/bulk-scan/app-insights-connection-string}",
  "role": {
    "name": "bulk-scan-processor"
  },
  "preview": {
    "sampling": {
      "overrides": [
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            },
            {
              "key": "name",
              "value": ".*run.*",
              "matchType": "regexp"
            },
            {
              "key": "name",
              "value": ".*process.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "dependency",
          "attributes": [
            {
              "key": "name",
              "value": ".*BlobStorage.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "dependency",
          "attributes": [
            {
              "key": "type",
              "value": "postgresql",
              "matchType": "exact"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "dependency",
          "attributes": [
            {
              "key": "name",
              "value": ".*ServiceBus.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "dependency",
          "attributes": [
            {
              "key": "type",
              "value": "Azure blob",
              "matchType": "exact"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryType": "trace",
          "attributes": [
            {
              "key": "name",
              "value": ".*run.*",
              "matchType": "regexp"
            },
            {
              "key": "name",
              "value": ".*process.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        }
      ]
    }
  }
}
